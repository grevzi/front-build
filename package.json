{
  "name": "front-build",
  "version": "1.0.0",
  "description": "Base Laravel Mix build",
  "main": "index.js",
  "scripts": {
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/grevzi/front-build.git"
  },
  "keywords": [
    "js",
    "sass",
    "laravel-mix",
    "frontend",
    "build"
  ],
  "author": "grevzi@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/grevzi/front-build/issues"
  },
  "homepage": "https://github.com/grevzi/front-build#readme",
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "alpinejs": "^2.8.0",
    "autoprefixer": "^9.8.6",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^4.5.1",
    "laravel-mix": "^5.0.9",
    "laravel-mix-clean": "^0.1.0",
    "laravel-mix-svg-sprite": "^1.0.2",
    "laravel-mix-twig-to-html": "^1.3.0",
    "lazysizes": "^5.3.0",
    "postcss": "^8.2.4",
    "raw-loader": "^4.0.2",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.32.5",
    "sass-loader": "^8.0.2",
    "swiper": "^6.4.11",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "twig-html-loader": "^0.1.8",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "laravel-mix-tailwind": "^0.1.1",
    "mix-tailwindcss": "^1.3.0",
    "twig": "^1.15.4"
  }
}
